Checks:          'clang-diagnostic-*,clang-analyzer-*,-*,cppcoreguidelines-*,concurrency-*,clang-analyzer-*,performance-*,portability-*,readability-*,-readability-redundant-access-specifiers'
WarningsAsErrors: '*'
HeaderFileExtensions:
  - ''
  - h
  - hh
  - hpp
  - hxx
ImplementationFileExtensions:
  - c
  - cc
  - cpp
  - cxx
HeaderFilterRegex: '.*'
FormatStyle:     none
User:            loic corenthy
CheckOptions:
  cert-dcl16-c.NewSuffixes: 'L;LL;LU;LLU'
